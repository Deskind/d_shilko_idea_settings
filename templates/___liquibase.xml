<templateSet group="+++liquibase">
  <template name="liquibase formatted sql" value="--liquibase formatted sql" description="for properties file" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="SQL" value="true" />
    </context>
  </template>
  <template name="liquibase changeset" value="--changeset &lt;author_name&gt;:&lt;change_set_description&gt;" description="change set comment" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="SQL" value="true" />
    </context>
  </template>
  <template name="liquibase properties file" value="changeLogFile: &lt;change_log_file_name&gt;&#10;&#10;url: jdbc:postgresql://localhost:5432/liqui_test&#10;username: postgres&#10;password: pgsuper&#10;liquibase.hub.mode=off" description="liquibase.properties" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="ANY_OPENAPI_JSON_FILE" value="false" />
      <option name="ANY_OPENAPI_YAML_FILE" value="false" />
      <option name="ASPECTJ" value="false" />
      <option name="CSS" value="false" />
      <option name="CUCUMBER_FEATURE_FILE" value="false" />
      <option name="CoffeeScript" value="false" />
      <option name="ECMAScript6" value="false" />
      <option name="GENERAL_JSON_FILE" value="false" />
      <option name="GENERAL_YAML_FILE" value="false" />
      <option name="GROOVY" value="false" />
      <option name="GSP" value="false" />
      <option name="HAML" value="false" />
      <option name="HTML" value="false" />
      <option name="JAVA_CODE" value="false" />
      <option name="JAVA_SCRIPT" value="false" />
      <option name="JSON" value="false" />
      <option name="JSP" value="false" />
      <option name="KOTLIN" value="false" />
      <option name="MAVEN" value="false" />
      <option name="OTHER" value="true" />
      <option name="REQUEST" value="false" />
      <option name="SHELL_SCRIPT" value="false" />
      <option name="SQL" value="false" />
      <option name="TypeScript" value="false" />
      <option name="XML" value="false" />
      <option name="XML_ATTRIBUTE" value="false" />
    </context>
  </template>
  <template name="luquibase changelog file" value="--liquibase formatted sql&#10;&#10;--changeset DmitryShilko:45545-addusertable&#10;create table liqui_schema.user(&#10;    id int generated by default as identity primary key,&#10;    firstname varchar(255),&#10;    lastname varchar(255)&#10;    );&#10;&#10;--rollback drop table liqui_schema.user;&#10;&#10;--changeset DmitryShilko:45545-insert_users&#10;insert into liqui_schema.user (firstname, lastname) VALUES ('John', 'Petrov');&#10;insert into liqui_schema.user (firstname, lastname) VALUES ('Pamella', 'Anderson');&#10;&#10;--rollback delete from liqui_schema.user where lastname = 'Petrov';&#10;--rollback delete from liqui_schema.user where lastname = 'Anderson';" description="changelog.postgresql.sql" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="SQL" value="true" />
    </context>
  </template>
  <template name="liquibase rollback comment" value="--rollback drop table liqui_schema.user;" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="SQL" value="true" />
    </context>
  </template>
  <template name="liquibase rollback counted" value="liquibase rollbackCount 1" description="to roll back last item" toReformat="false" toShortenFQNames="true" />
</templateSet>